# CS2 Trade-Up Educational Bot - Environment Variables Template
# Copy this file to .env and fill in your actual values
# NEVER commit .env with real credentials to version control

# Steam Authentication
STEAM_USERNAME=your_steam_username_here
STEAM_PASSWORD=your_steam_password_here
STEAM_SHARED_SECRET=your_steam_shared_secret_here
STEAM_API_KEY=your_steam_api_key_here

# Application Configuration
NODE_ENV=development
LOG_LEVEL=info

# Optional: Steam Guard Code (if not using shared secret)
# STEAM_GUARD_CODE=XXXXX

# Optional: Trade Configuration
# MAX_DAILY_CRAFTS=12
# MAX_MONTHLY_CRAFTS=100
# CRAFT_DELAY_MIN=300000
# CRAFT_DELAY_MAX=900000

# Optional: Data Directory (override default /data)
# DATA_DIR=/data

# Optional: Port for health checks (future use)
# HEALTH_CHECK_PORT=3000
